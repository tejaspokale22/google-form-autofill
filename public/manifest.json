{
  "manifest_version": 3,
  "name": "Autofill Google Forms",
  "version": "1.3.0",
  "description": "a chrome extension to autofill repetitive google forms.",
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["https://docs.google.com/forms/*"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Google Form Autofill"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Open Autofill Google Forms extension"
    }
  }
}
